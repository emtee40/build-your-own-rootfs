Version: 1.6.34
Fetch: ftp://ftp.simplesystems.org/pub/libpng/png/src/libpng16/libpng-{Version}.tar.xz
Signature: ftp://ftp.simplesystems.org/pub/libpng/png/src/libpng16/libpng-{Version}.tar.xz.asc

Build:
 - ./configure --prefix=/usr --host={Host} CPPFLAGS=-I{InstallDirectory}/usr/include LDFLAGS=-L{InstallDirectory}/usr/lib
 - make
 - make install DESTDIR={InstallDirectory}
