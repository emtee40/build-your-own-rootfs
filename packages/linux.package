Version: 4.16.14
Fetch: https://cdn.kernel.org/pub/linux/kernel/v4.x/linux-{Version}.tar.xz
Signature: https://cdn.kernel.org/pub/linux/kernel/v4.x/linux-{Version}.tar.sign

Build:
 - ARCH={LinuxPlatform} CROSS_COMPILE={CrossCompilePrefix} make vexpress_defconfig
 - ARCH={LinuxPlatform} CROSS_COMPILE={CrossCompilePrefix} make -j{Jobs}
 - mkdir -p {InstallDirectory}/boot
 - ARCH={LinuxPlatform} CROSS_COMPILE={CrossCompilePrefix} make zinstall dtbs_install headers_install modules_install INSTALL_HDR_PATH={InstallDirectory}/usr INSTALL_MOD_PATH={InstallDirectory} INSTALL_PATH={InstallDirectory}/boot INSTALL_DTBS_PATH={InstallDirectory}/boot
